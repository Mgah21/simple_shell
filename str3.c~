#include "shell.h"

/**
 * _atoi - atoi
 * Return:num
 */
int _atoi(void)
{
int c;
int d;
int e;
int f;
char *st;
c = e = 0;
f = 1;
while ((st[c] < '0' || st[c] > '9') && (st[c] != '\0'))
{
if (st[c] == '-')
f *= -1;
c++;
}
d = c;
while ((st[d] >= '0') && (st[d] <= '9'))
{
e = (e * 10) + f * ((st[d]) - '0');
d++;
}
return (e);
}
